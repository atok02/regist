<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="411dp"
        android:layout_height="840dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/register__step_1_" />

    <View
        android:id="@+id/rectangle_2_ek2"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:layout_marginTop="446dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    <RelativeLayout
        android:id="@+id/status_bar"
        android:layout_width="381dp"
        android:layout_height="48dp"
        tools:ignore="MissingConstraints">


        <RelativeLayout
            android:id="@+id/connections"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="293dp"
            android:layout_marginTop="15dp">


            <RelativeLayout
                android:id="@+id/battery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="43dp"
                android:layout_marginTop="2dp">

                <View
                    android:id="@+id/border"
                    android:layout_width="24dp"
                    android:layout_height="13.33dp"
                    android:layout_marginStart="0.33dp"
                    android:layout_marginTop="0.33dp"
                    android:alpha="0.35"
                    android:background="@color/white" />

                <ImageView
                    android:id="@+id/cap"
                    android:layout_width="1.33dp"
                    android:layout_height="4dp"
                    android:layout_marginStart="23dp"
                    android:layout_marginTop="4dp"
                    android:alpha="0.40000000596046"
                    android:contentDescription="cap" />

                <View
                    android:id="@+id/capacity"
                    android:layout_width="18dp"
                    android:layout_height="7.33dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginTop="2.33dp"
                    android:background="@color/white" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/wifi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="1dp">

                <ImageView
                    android:id="@+id/wifi_ek1"
                    android:layout_width="15.33dp"
                    android:layout_height="11dp"
                    android:layout_marginTop="1.33dp"
                    android:contentDescription="wifi_ek1" />

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/cellular_connection"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="2dp">

                <ImageView
                    android:id="@+id/cellular_connection_ek1"
                    android:layout_width="17dp"
                    android:layout_height="10.67dp"
                    android:layout_marginStart="0.67dp"
                    android:layout_marginTop="0.67dp"
                    android:contentDescription="cellular_connection_ek1" />

            </RelativeLayout>

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="21dp"
            android:layout_marginTop="8dp">

            <TextView
                android:id="@+id/time_ek1"
                android:layout_width="67dp"
                android:layout_height="28.5dp"
                android:layout_gravity="center_horizontal|center"
                android:layout_marginLeft="-2.67dp"
                android:layout_marginTop="6dp"
                android:backgroundTint="#FFFFFF"
                android:fontFamily="SF Pro Text"
                android:gravity="center"
                android:textSize="15sp"
                android:textStyle="bold" />

        </RelativeLayout>

    </RelativeLayout>

    <View
        android:id="@+id/rectangle_2_ek"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="382dp" />

    <View
        android:id="@+id/rectangle_"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:layout_marginBottom="20dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.986" />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:layout_marginTop="247dp"
        android:layout_marginBottom="12dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:layout_marginTop="56dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rectangle_2_ek"
        app:layout_constraintVertical_bias="0.022" />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="347dp"
        android:layout_height="56dp"
        android:layout_marginTop="315dp"
        android:layout_marginBottom="11dp"
        android:background="@color/white"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>